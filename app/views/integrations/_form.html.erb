= form_for @integration do |f|
  - if @integration.errors.any?
    #error_explanation
      %h2= "#{pluralize(@integration.errors.count, "error")} prohibited this integration from being saved:"
      %ul
        - @integration.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :type_file
    = f.text_field :type_file
  .actions
    = f.submit 'Save'
